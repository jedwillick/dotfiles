{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "pwd": "osc99",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "{{.Icon}} ",
          "foreground": "#ffffff",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}@{{ .HostName }}<#ffffff>:</>",
          "foreground": "#DDB15F",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "hide_root_location": false,
            "mapped_locations": {
              "/mnt/c": "C:"
            },
            "mapped_locations_enabled": true,
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "template": "{{ .Path }}",
          "foreground": "#61afef",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": " ({{ .HEAD }})",
          "foreground": "lightRed",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if eq .OS \"windows\"}}‚ùØ{{ else }}${{ end }}",
          "foreground": "#ffffff",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if ne .Code 0 }}#e91e63{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
