{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "darkGray",
          "style": "plain",
          "template": "\u250c",
          "type": "text"
        },
        {
          "foreground": "#61afef",
          "properties": {
            "mapped_locations": {
              "/mnt/c": "C:"
            },
            "hide_root_location": false,
            "mapped_locations_enabled": true,
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#DDB15F",
          "properties": {
            "branch_ahead_icon": "<#D4AAFC>\u2191</>",
            "branch_behind_icon": "<#D4AAFC>\u2193</>",
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " \u27e8{{ if .UpstreamURL }}{{ url .HEAD .UpstreamURL }}{{ else }}{{ .HEAD }}{{ end }}{{.BranchStatus}}{{ if .Staging.Changed }} <#D4AAFC>*</>{{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} <#D4AAFC>!</>{{ .Working.String }}{{ end }}\u27e9",
          "type": "git"
        },
        {
          "type": "svn",
          "style": "plain",
          "foreground": "#DDB15F",
          "properties": {
            "fetch_status": true
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#100e23",
          "background": "#906cff",
          "template": " \ue235 {{ .Full }} ",
          "properties": {
            "display_mode": "context"
          }
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": " {{ if le .Percentage 20 }}{{ if ne .State 3 }}{{ .Percentage }}%{{ end }} {{ end }}",
          "type": "battery"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#e91e63",
          "style": "plain",
          "template": "{{ .Code }} ",
          "type": "exit"
        },
        {
          "foreground": "#A3BE8C",
          "properties": {
            "style": "austin",
            "threshold": 500
          },
          "style": "plain",
          "template": "{{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "foreground": "#689f38",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }}",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "darkGray",
          "style": "plain",
          "template": "\u2514 ",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "foreground_templates": ["{{ if ne .Code 0 }}#e91e63{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if eq .OS \"windows\"}}\u276f{{ else }}${{ end }}",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} :: {{ .Folder }}",
  "final_space": true,
  "version": 2
}
